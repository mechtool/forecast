{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "dist/images/forecast-icons"
    ],
    "rewrites": [
      {
        "source": "/apiKey",
        "function": "apiKey"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
